<Window x:Class="auapp.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:auapp"
        mc:Ignorable="d"
        Title="Register" Height="450" Width="800">
    <Grid>
        <Label Content="Register"  HorizontalAlignment="Left" Margin="24,24,0,0" VerticalAlignment="Top" Width="138" Height="34"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="177,153,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="UserName" HorizontalAlignment="Left" Height="23" Margin="272,156,0,0" TextWrapping="Wrap" Text="{Binding UserName}" VerticalAlignment="Top" Width="120"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="177,203,0,0" VerticalAlignment="Top" Width="108"/>
        <TextBox x:Name="Password" HorizontalAlignment="Left" Height="23" Margin="272,206,0,0" TextWrapping="Wrap" Text="{Binding UserPass}" VerticalAlignment="Top" Width="120"/>
        <Button  Content="Submit" Click="Button_Click" HorizontalAlignment="Left" Margin="177,264,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Login Page" Click="LoginPage" HorizontalAlignment="Left" Margin="317,264,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
